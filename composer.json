{
    "name": "cdm14cn/yii2-wallaby",
    "description": "Wallaby Project Template",
    "keywords": ["yii2","Wallaby", "framework", "advanced", "project template"],
    "homepage": "http://www.sdaishu.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/cdm14cn/yii2-wallaby/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/cdm14cn/yii2-wallaby"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "mdmsoft/yii2-admin": "*",
        "mootensai/yii2-enhanced-gii": "dev-master",
        "yiisoft/yii2-imagine": "^2.0",
        "laravel/framework": "5.1.*",
        "topthink/thinkphp": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 18000
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
